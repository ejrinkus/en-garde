<head>
  <link rel="stylesheet" href="/styles/styles.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="/js/jquery-3.6.0.js"><\/script>')</script>
  <script src="/js/navbar.js"></script>
  <script src="/js/form.js"></script>
</head>
<body>
  <nav id="titlebar"></nav>
  <div id="main" onclick="closeNav()">
    <h1 class="centered">Players</h1>
    <table class="centered">
      <tr>
        <th>ID</th>
        <th>Name</th>
      </tr>
      {{#each player_list}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.name}}</td>
      </tr>
      {{/each}}
      <tr>
        <td></td>
        <td>
          <form id="addPlayerForm">
            <input id="playerName" name="name" type="text" required="required" maxlength="100"></input>
            <input id="newPlayerSubmit" type="submit" value="Add"></input>
          </form>
        </td>
      </tr>
    </table>
  </div>
</body>